  <tag>
    <name>list</name>
    <tag-class>org.makumba.list.tags.QueryTag</tag-class>
    <tei-class>org.makumba.list.tags.CountTEI</tei-class>
    <body-content>JSP</body-content>
    <display-name>manipulate lists</display-name>
    

    <attribute>
      <name>from</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
      <description>OQL FROM section. If the query is nested, the keys from the parent's query are added to this FROM
        section in the resulting query</description>
      
    </attribute>

    <attribute>
      <name>variableFrom</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>supplementary, variable OQL FROM section.</description>
      
    </attribute>

    <attribute>
      <name>where</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>OQL WHERE section. If the query is nested, the WHERE section from the parent's query, is added to
        this in the resulting query</description>
      
    </attribute>

    <attribute>
      <name>orderBy</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>OQL ORDER BY section</description>
      
    </attribute>

    <attribute>
      <name>groupBy</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>OQL GROUP BY section</description>
      
    </attribute>

    <attribute>
      <name>id</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>Tag's identifier</description>
      
    </attribute>

    <attribute>
      <name>db</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The makumba database to connect to. The format is Host_SQLEngineType_DatabaseName
        If not present, the default database is used. See database configuration and lookup.</description>
      
    </attribute>

    <attribute>
      <name>offset</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>the offset of the result to show (0 for starting from the first result)</description>
      
    </attribute>

    <attribute>
      <name>limit</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>the maximum numbers of results to show (-1 for all results)</description>
      
    </attribute>

    <attribute>
      <name>defaultLimit</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>the default value for limit, if the $attribute in limit="$attribute" is not present.</description>
      
    </attribute>

    <attribute>
      <name>authorize</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>Document me please</description>
      
    </attribute>

    <attribute>
      <name>editable</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Document me please</description>
      
    </attribute>

    <attribute>
      <name>logicClass</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Document me please</description>
      
    </attribute>

    <attribute>
      <name>editPage</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Document me please</description>
      
    </attribute>

    <attribute>
      <name>countVar</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>The java.lang.Integer variable that will contain the number of the current iteration.</description>
      
      
    </attribute>

    <attribute>
      <name>maxCountVar</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
      <description>The java.lang.Integer variable that will contain the total number of iterations.</description>
      
      
    </attribute>

    <attribute>
      <name>separator</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>Character string as separator between two list elements. It is not included before the first or after
        the last</description>
      
      
    </attribute>

  </tag>


