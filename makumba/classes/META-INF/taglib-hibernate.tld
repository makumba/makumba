  <tag>
    <name>newForm</name>
    <tag-class>org.makumba.forms.tags.NewTag</tag-class>
    <body-content>JSP</body-content>
    <display-name>Forms to create new data</display-name>
    

    <attribute>
      <name>type</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
      <description>Makumba type for record creation, or OQL label for subrecord creation</description>
      
    </attribute>

    <attribute><name>action</name><required>false</required><rtexprvalue>true</rtexprvalue><description>the response page of the form</description></attribute>
    <attribute><name>method</name><required>false</required><rtexprvalue>true</rtexprvalue><description>the HTTP method (get or post)</description></attribute>

    <attribute>
      <name>name</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>the name of the created object in the response page</description>
      
    </attribute>

    <attribute><name>message</name><required>false</required><rtexprvalue>true</rtexprvalue><description>the message that is displayed in the response page upon success</description></attribute>
    <attribute><name>db</name><required>false</required><rtexprvalue>true</rtexprvalue><description>The makumba database to connect to. The format is Host_SQLEngineType_DatabaseName
        If not present, the default database is used. See database configuration and lookup.</description></attribute>
    <attribute><name>annotation</name><required>false</required><rtexprvalue>true</rtexprvalue><description><![CDATA[Indicates that validation whether errors should be shown in the form, or be displayed in the <mak:response/>.]]></description></attribute>
    <attribute><name>annotationSeparator</name><required>false</required><rtexprvalue>true</rtexprvalue><description>Fixed. Prints the given separator between the annotation and the input field.</description></attribute>
    <attribute><name>reloadFormOnError</name><required>false</required><rtexprvalue>true</rtexprvalue><description>Indicates whether the form page shall be reloaded on an error, or the specified form action page will
        be loaded directly.</description></attribute>
    <attribute><name>clientSideValidation</name><required>false</required><rtexprvalue>true</rtexprvalue><description>Controls whether client side validation shall be used.</description></attribute>
    <attribute><name>multipleSubmitErrorMsg</name><required>false</required><rtexprvalue>true</rtexprvalue><description>Enables multiple form submission prevention, and sets the given value as the error message.
      </description></attribute>

